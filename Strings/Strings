#include <iostream>
#include <string>
using namespace std;
int main()
{
    

    string first;
    string second;

    cin >> first >> second;

    cout << first.size()   << ' '
         << second.size()  << '\n'
         << first + second << '\n';

    swap(first.front(), second.front());

    cout << first  << " "<< second << '\n';

    return 0;
}
